<?xml version="1.0" ?>

<configuration>
	<log-level>DEBUG</log-level>
	<cache>
		<cache-dir>testdata/test_cache</cache-dir>
		<source-dir>testdata/test_root</source-dir>
		<checksum-function>sha1</checksum-function>
		<max-age>86400</max-age>
		<max-entries>2048</max-entries>
		<auto-scrub />
		<send-etags />
	</cache>
	<processors>
		<encoding>utf8</encoding>
		<processor>asciidoc-xhtml11</processor>
		<processor mime-type="image/png" extensions = "png">raw</processor>
		<processor mime-type="image/jpeg" extensions = "jpg jif jfif jpeg">raw</processor>
		<processor mime-type="image/gif" extensions = "gif">raw</processor>
		<processor mime-type="image/bmp" extensions = "bmp">raw</processor>
	</processors>
</configuration>
